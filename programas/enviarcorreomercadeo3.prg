	STORE '' TO IERRORES
	FOR I=1 TO LOMAIL.GETERRORCOUNT()
		IERRORES=IERRORES+ LOMAIL.GETERROR(I)
	ENDFOR
	IF 'NOT AVAILABLE'$UPPER(IERRORES)
		MESSAGEBOX('No se pudo enviar el Correo, Usuario o Contraseña Incorrecta.',0+16+0,'Error')
