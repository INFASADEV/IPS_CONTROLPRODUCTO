		MESSAGEBOX('No se pudo enviar el Correo.  '+CHR(13)+CHR(13)+IERRORES,0+16+0,'Error')
	ENDIF
	LOMAIL.CLEARERRORS()
	RETURN .F.
ELSE
	LOMAIL.CLEARERRORS()
	WAIT WINDOW "Corren enviado correctamente" TIMEOUT 1
	RETURN .T.
